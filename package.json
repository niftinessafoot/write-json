{
  "name": "@afoot/write-json",
  "version": "0.1.2",
  "description": "Parse and write json to the filesystem.",
  "scripts": {
    "prebuild": "npm run clean",
    "build": "rollup -c rollup.config.js",
    "dev": "npm run build -- -w",
    "prepare": "npm run build",
    "clean": "rm -rf dist/*",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:coverage": "npm run test -- --coverage",
    "postversion": "git push && git push --tags"
  },
  "type": "module",
  "main": "dist/umd/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "require": "./dist/umd/index.js",
      "import": "./dist/esm/index.js",
      "default": "./dist/umd/index.js"
    }
  },
  "files": [
    "dist/"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/niftinessafoot/write-json.git"
  },
  "homepage": "https://github.com/niftinessafoot/write-json",
  "keywords": [
    "javascript",
    "module",
    "package.json"
  ],
  "author": "Matthew Smith @niftinessafoot",
  "license": "MIT",
  "devDependencies": {
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "rollup": "^4.13.2"
  },
  "browserslist": [
    "defaults"
  ]
}
